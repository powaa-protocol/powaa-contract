    "hardhat-typechain": "0.3.5",
    "prettier": "2.5.1",
    "prettier-plugin-solidity": "1.0.0-beta.19",
    "solhint": "3.3.7",
    "solidity-coverage": "0.7.21",
    "ts-node": "10.8.1",
    "typechain": "8.1.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@makerdao/hardhat-utils": "0.1.3",
    "@uniswap/swap-router-contracts": "^1.3.0",
    "@uniswap/v3-periphery": "^1.4.1"
  },
  "files": [
    "typechain",
    "artifacts",
    "compiled-typechain"
  ],
  "engines": {
    "node": ">=14.16.1"
  }
}
